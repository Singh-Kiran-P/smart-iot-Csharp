<Window x:Class="Smartiot.Views.Dialog.server_setup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="Smartiot.Views.Dialog"
        mc:Ignorable="d"
        Title="server_setup" Height="139.428" Width="354.807">
    <Grid Margin="0,0,-2,-1">
        <Button x:Name="btn_change" Content="Change" HorizontalAlignment="Left" Height="21" Margin="132,54,0,0" VerticalAlignment="Top" Width="91" Click="btn_change_Click"/>
        <TextBox x:Name="txt_url" HorizontalAlignment="Left" Height="23" Margin="58,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="274" TextChanged="txt_url_TextChanged" />
        <Label x:Name="label" Content="API URL:" HorizontalAlignment="Left" Margin="3,7,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btn_return" Content="return" HorizontalAlignment="Left" Height="18" Margin="294,81,0,0" VerticalAlignment="Top" Width="38" Click="btn_return_Click" />

    </Grid>
</Window>
